    \begin{definition}[正則グラフ]
      任意のノードの次数$r$であるグラフを$r$-正則グラフという。
    \end{definition}

    \begin{definition}[完全グラフ]
      任意のノードが、自分以外の任意のノードと隣接するグラフを完全グラフという。
    \end{definition}

    \begin{definition}[2部グラフ]
      ある集合$V_1, V_2$と、$V_1, V_2$から定まる$E \subset (V_1 \times V_2 \cup V_2 \times V_1)$によるグラフ$G = (V_1 \cup V_2, E)$を2部グラフという。
    \end{definition}

    \begin{definition}[道(path)、閉路(cycle)]
      あるグラフ$G = (V, E)$と自然数$k$に対して$k+1$個のノード$v_0, v_1, \ldots, v_k \in V$と$k$個の辺$e_1, e_2, \ldots, e_k$からなる交代列
      \begin{equation}
        (v_0, e_1, v_1, e_2, v_2, \ldots, e_k, v_k)
      \end{equation}
      を長さ$k$の道という。道の$v_0$を始点(起点)、$v_k$を終点という。
      また、$v_0 = v_k$である歩道を閉路という。

      歩道や道、閉路を表す交代列に$v \in V$や$e \in E$が含まれるとき
      \begin{itemize}
        \item 歩道(道、閉路)はノード$v$や辺$e$を経路上に持つ
        \item 歩道(道、閉路)はノード$v$や辺$e$を通る
        \item ノード$v$や辺$e$は歩道(道、閉路)上にある
      \end{itemize}
      等という。
    \end{definition}

    \begin{definition}[単純な道・初等的な道]
      同じ辺を通らない道を単純な道、同じ点を通らない道を初等的な道という。
    \end{definition}
    \begin{theorem}
      ある道$P$が初等的な道ならば$P$は単純な道である。
    \end{theorem}

    \begin{definition}[有向路]
      有向グラフ$G = (V, E)$上の道$P$が
      \begin{equation}
        (v_0, e_1, v_1, e_2, v_2, \ldots, e_k, v_k)
      \end{equation}
      と書かれるとき、任意の$1 \le i < k$に対して$e_i = (v_{i-1}, v_i)$が成り立つならば道$P$を有向な道、有向路という。
    \end{definition}

    道に対する単純な道、初等的な道、有向路と同様に閉路に対しても単純な閉路、初等的な閉路、有向閉路が定まる。% TODO

    \begin{definition}[タイセット]
      グラフ$G = (V, E)$のある道$P$が
      \begin{equation}
        (v_0, e_1, v_1, e_2, v_2, \ldots, e_k, v_k)
      \end{equation}
      と書かれるとき、この道$P$を
      \begin{equation}
        (e_1, e_2, \ldots, e_k)
      \end{equation}
      と書くかきかたをタイセットという。
      有向グラフの場合は辺の向きに対して経路の向きの正負を記入する。
    \end{definition}

    \begin{definition}[カットセット]
      あるグラフ$G = (V, E)$のノードの集合$V$を$X$と$\overline{X}$に分割したとき、$X$のノードと$\overline{X}$のノードを繋ぐ辺の集合をカットセットといい$C(X, \overline{X})$と書く。
    \end{definition}


